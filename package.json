  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:rebuild": "cd .. && docker-compose -f docker-compose.yml -f docker-compose.dev.yml stop btd-payment && docker-compose -f docker-compose.yml -f docker-compose.dev.yml rm -f btd-payment && docker-compose -f docker-compose.yml -f docker-compose.dev.yml build --no-cache btd-payment && docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d btd-payment",
    "docker:build": "cd .. && docker-compose -f docker-compose.yml -f docker-compose.dev.yml build btd-payment",
    "docker:logs": "cd .. && docker-compose -f docker-compose.yml -f docker-compose.dev.yml logs -f btd-payment"
  },
